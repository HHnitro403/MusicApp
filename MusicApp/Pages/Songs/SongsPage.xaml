<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MusicApp.Pages.Songs.SongsPage"
             xmlns:controls="clr-namespace:MusicApp.Components"
             Title="SongsPage">
    <Shell.BackButtonBehavior>
        <BackButtonBehavior Command="{Binding UnifiedBackCommand}"
                        IsEnabled="{Binding IsBackEnabled}"
                        IsVisible="{Binding IsBackVisible}" />
    </Shell.BackButtonBehavior>

    <VerticalStackLayout>
        <Label
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center"
            HorizontalOptions="Center"
            x:Name="lblWelcome" />
    </VerticalStackLayout>
</ContentPage>